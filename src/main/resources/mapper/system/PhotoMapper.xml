<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lzxuni.modules.system.mapper.PhotoMapper">

	<!-- 根据部门id查询部门 -->
	<select id="queryListPhtotAndQszqtp"  resultType="com.lzxuni.modules.system.entity.zlzs.PhotoCustom">
		select a.*,b.urlPath as qszqtp from tbl_zlzs_photo a
		LEFT JOIN sys_file b ON a.id=b.ywId
	</select>

</mapper>