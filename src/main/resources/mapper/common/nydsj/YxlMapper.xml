<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lzxuni.modules.common.mapper.nydsj.YxlMapper">
    <select id="findmohuYxl" resultType="com.lzxuni.modules.common.entity.nydsj.Yxl" parameterType="com.lzxuni.modules.common.entity.nydsj.Yxl">
          SELECT * FROM tbl_nydsj_yxl WHERE  phosphorus LIKE CONCAT(CONCAT('%', #{phosphorus}), '%') OR region LIKE CONCAT(CONCAT('%', #{region}), '%')
    </select>
    <select id="queryList" resultType="com.lzxuni.modules.common.entity.nydsj.Yxl" parameterType="com.lzxuni.modules.common.entity.nydsj.Yxl">
        SELECT * FROM tbl_nydsj_yxl
    </select>
</mapper>